{
  "name": "astra",
  "description": "Astra is a React component library that is accessible by default. It is built with Tailwind CSS and React Aria Components.",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "author": {
    "name": "Rahul SriRam",
    "email": "hi@rahulsriram.dev",
    "url": "https://rahulsriram.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theskinnycoder/astra.git"
  },
  "bugs": {
    "url": "https://github.com/theskinnycoder/astra/issues"
  },
  "keywords": [
    "react",
    "accessible",
    "component-library"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react-aria-components": "^1.3.1",
    "tailwind-merge": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "postcss": "^8.4.40",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.4.7",
    "tailwindcss-react-aria-components": "^1.1.4",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
